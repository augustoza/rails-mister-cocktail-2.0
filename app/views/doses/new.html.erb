<div class="form-wrapper">
  <div class="form">
    <h2>Create a new cocktail!</h2>
    <%= simple_form_for [ @cocktail, @dose ] do |f| %>
      <%= f.input :description %>
      <%= f.association :ingredient %>
      <%= f.submit "Submit ingredient", class: "btn btn-form" %>
    <% end %>
  </div>
</div>